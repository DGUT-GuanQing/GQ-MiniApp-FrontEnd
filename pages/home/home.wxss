/* pages/home/home.wxss */
.wrapperBox{
  box-sizing: border-box;
}
.titlewrapper{
  
  display: flex;
  margin-left: 30rpx;
  /* margin-top: 20rpx; */
}
.square{
  width: 15rpx;
  height: 44rpx;
  background: rgba(103, 199, 143, 1);
}
.title{
  margin-left: 15rpx;
  font-size: 34rpx;
  font-weight: 400;
}
.tweet{
  box-sizing: border-box;
  margin:9rpx 30rpx 45rpx 30rpx;
}
.center{
  box-sizing: border-box;
  overflow: hidden;
  width: 690rpx;
  height: 320rpx;
  margin: 10rpx 30rpx 30rpx 30rpx;
  background-color: aqua;
  /* background-image: url("https://img.js.design/assets/img/63e373ee9aaf9b18c59ee160.png#73b7a28ff19522b60c098cd32ffb9329") ; */
  background-size:cover;
  padding: 0rpx 30rpx;
  border-radius: 20rpx;
  background: rgba(234, 250, 240, 0.2);
  border: 1rpx solid rgba(255, 255, 255, 1);
  box-shadow: 2rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.25), -2rpx -2rpx 4rpx 0rpx rgba(163, 163, 163, 0.4);
}
.center .wrapperSign{
  display: flex;
  justify-content: center;
}
.center .button{
  margin-top: 20rpx;
  text-align: center;
  width: 165rpx;
  height: 55rpx;
  box-sizing:content-box;
  /* background: linear-gradient(226.74deg, #50eb83 30.47%, #78df59 72.74%); */
  /* background: linear-gradient(90deg, rgba(102, 232, 135, 1) 0%, rgba(102, 232, 132, 1) 3.61%, rgba(120, 223, 89, 1) 100%); */
  border-radius: 20rpx;
  background: rgba(103, 199, 143, 1);
  border: 6rpx solid rgba(255, 255, 255, 1);
  box-shadow: 0px 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.25);
}
.center .button text{
  font-size: 32rpx;
  font-weight: 700;
  line-height:55rpx;
  color: rgba(255, 255, 255, 1);
  text-align: left;
  vertical-align: top;
  text-shadow: 0px 2rpx 4rpx 0px rgba(0, 0, 0, 0.25);
}


.wrapperFunction{
  display: flex;
  width: 100%;
  justify-content: space-between;
  margin-top: 50rpx;
}
.wrapperFunction .FunctionButton{
  display: flex;
  flex-direction: column;
  align-items: center;
  color: rgba(1, 2, 27, 0.592);
  
  font-weight: 800;
  font-size: 30rpx;
}
.wrapperFunction image{
  margin-bottom: 20rpx;
  width: 110rpx;
  height: 110rpx;
}


.masterWrapper{
  box-sizing: border-box;
  width: 690rpx;
  height: 550rpx;
  margin: 45rpx 30rpx 30rpx 30rpx;
  /* background-image: url("https://img.js.design/assets/img/63e53115c83ced6dc23c9e0c.png#336384cecfce60c23b13ae4c89acac34"); */
  background-image: url("https://img.js.design/assets/img/63e374d7653751857e2ee3b2.png#c500814de6c187c00c73cccc143ae1a0");
  /* background-color: blanchedalmond; */
  background-size: cover;
  border-radius: 24rpx;
  /* padding: 30rpx; */
  display: flex;
  flex-direction: column;
  box-shadow: 0rpx 2rpx 20rpx 0rpx rgba(0, 0, 0, 0.02);
}
.masterWrapper .dakaTitle {
  box-sizing: border-box;
  font-size: 36rpx;
  font-weight: 600;
  color: rgba(125, 101, 101, 1);
}


.masterWrapper .scrollView{
  box-sizing: border-box;
  height: 390rpx;
  border-radius: 24rpx;
}
.master{
  box-sizing: border-box;
  margin-top:20rpx;
  margin-left: 30rpx;
}