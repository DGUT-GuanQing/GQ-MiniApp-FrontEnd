/* component/master/master.wxss */
.wrapper{
  box-sizing: border-box;
  width: 630rpx;
  /* height: 170rpx; */
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx;
  background: rgb(255, 255, 255);
  border-radius: 24rpx;
  box-shadow: 0px 2px 20px 0px rgba(0, 0, 0, 0.02);
}
.wrapper .image{
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}
.wrapper .identity{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: -120rpx;
  height: 80rpx;
}
.identity .text1{
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(3, 163, 43, 1);
}
.identity .text2{
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(173, 173, 178, 1)
}
.time{
  font-weight: 600;
  color: rgba(1, 2, 27, 0.592);
}